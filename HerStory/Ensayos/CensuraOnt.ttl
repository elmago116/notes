# ===================================================================
# PROPERTY RESTRICTIONS AND AXIOMS
# ===================================================================

# Class Restrictions
:Autor rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :tituloNombreAutor ;
    owl:someValuesFrom xsd:string
] .

:LibrosPresentados rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :titulo ;
    owl:someValuesFrom xsd:string
] .

:Expedientes rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :tituloExpediente ;
    owl:someValuesFrom xsd:string
] .

:InformesLectores rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :esEscritoPor ;
    owl:someValuesFrom :Lectores
] .

# ===================================================================
# INVERSE PROPERTY DECLARATIONS
# ===================================================================

:presenta owl:inverseOf :esPresentadoPor .
:publica owl:inverseOf :esPublicadoPor .
:estaEn owl:inverseOf :define .
:solicitadoPor owl:inverseOf :solicita .
:esCompuestoPor owl:inverseOf :esEscritoPor .
:perteneceA owl:inverseOf :agrupa .
:publicadoPor owl:inverseOf :publicaImportacion .
:proveidoPor owl:inverseOf :provee .

