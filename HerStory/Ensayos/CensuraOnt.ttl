# ===================================================================
# CENSURA ONTOLOGY - Literary Censorship During Spanish Civil War
# ===================================================================

<http://HerStory.org/censura> a owl:Ontology ;
    dc:title "CensuraOnt - Literary Censorship Ontology (Cultura/Censura Database)"@en ,
             "CensuraOnt - Ontología de Censura Literaria (Base de Datos Cultura/Censura)"@es ,
             "CensuraOnt - Ontologia de Censura Literària (Base de Dades Cultura/Censura)"@ca ;
    dc:description "Ontology representing the literary censorship system during the Francoist period, with focus on Russian and Eastern European literature reception and translation. Based on the Cultura/Censura database from the CeLRu project."@en ,
                   "Ontología que representa el sistema de censura literaria durante el período franquista, con foco en la recepción y traducción de literatura rusa y de Europa Oriental. Basada en la base de datos Cultura/Censura del proyecto CeLRu."@es ,
                   "Ontologia que representa el sistema de censura literària durant el període franquista, amb focus en la recepció i traducció de literatura russa i d'Europa Oriental. Basada en la base de dades Cultura/Censura del projecte CeLRu."@ca ;
    dc:creator "Miquel Centelles Velilla" , "Ivan Garcia Sala" ;
    dc:contributor "HerStory Project - University of Barcelona" ;
    dcterms:created "2021-09-01"^^xsd:date ;
    dcterms:modified "2025-01-15"^^xsd:date ;
    dcterms:publisher "University of Barcelona - Faculty of Information and Audiovisual Media" ;
    dcterms:subject "Literary Censorship", "Francoist Spain", "Russian Literature", "Eastern European Literature", "Translation Studies", "Cultural Heritage", "Digital Humanities" ;
    dcterms:type "Ontology" ;
    dcterms:language "en", "es", "ca" ;
    dcterms:rights "Creative Commons Attribution 4.0 International" ;
    dcterms:source "Archivo General de la Administración Española - Expedientes de censura de libros franquista" , "Biblioteca Nacional Española" ;
    dcterms:coverage "Spain, 1936-1975" ;
    dcterms:identifier "http://HerStory.org/censura" ;
    dcterms:isPartOf "La censura franquista y la literatura rusa (1936-1966) - CeLRu Project PID2020-116868GB-I00" ;
    dcterms:rightsHolder "Ivan Garcia Sala (IP de CeLRu)" ;
    dcterms:audience "Scholars of Francoist censorship and reception/translation of Eastern European literatures" ;
    dcterms:extent "~4,000 estimated censorship files related to Eastern European literature from ~400,000 total files" ;
    dcterms:temporal "Database creation: 2021-09-01 / Expected stabilization: 2025-09-01" ;
    dcterms:provenance "30% of Eastern European files collected, database import recently initiated" ;
    dcterms:accessRights "Public access via Cultura/Censura website with user email identification" ;
    dcterms:conformsTo <https://5stardata.info/en/> ;
    owl:versionInfo "1.0" ;
    rdfs:seeAlso <https://cultura.cercador.cultura.gencat.cat/> , <http://www.cultura-censura.org/> .

# ===================================================================
# CLASSES - Main Entities
# ===================================================================

# Core Entities
:Autor a owl:Class ;
    rdfs:label "Author"@en, "Autor"@es, "Autor"@ca ;
    rdfs:comment "Person who writes or has written literary works subject to censorship review."@en .

:LibrosPresentados a owl:Class ;
    rdfs:label "Presented Books"@en, "Libros Presentados"@es, "Llibres Presentats"@ca ;
    rdfs:comment "Books submitted for censorship review before publication or importation."@en .

:LibrosPublicados a owl:Class ;
    rdfs:label "Published Books"@en, "Libros Publicados"@es, "Llibres Publicats"@ca ;
    rdfs:comment "Books that have been published and may be subject to post-publication review."@en .

:Expedientes a owl:Class ;
    rdfs:label "Files"@en, "Expedientes"@es, "Expedients"@ca ;
    rdfs:comment "Administrative files containing documentation related to censorship cases."@en .

:SolicitudesDeImportacion a owl:Class ;
    rdfs:label "Import Requests"@en, "Solicitudes de Importación"@es, "Sol·licituds d'Importació"@ca ;
    rdfs:comment "Formal requests for importing books or publications from abroad."@en .

:Importadores a owl:Class ;
    rdfs:label "Importers"@en, "Importadores"@es, "Importadors"@ca ;
    rdfs:comment "Individuals or organizations requesting to import books or publications."@en .

:Lectores a owl:Class ;
    rdfs:label "Censors"@en, "Lectores"@es, "Lectors"@ca ;
    rdfs:comment "Official censors responsible for reviewing and evaluating literary works."@en .

:InformesLectores a owl:Class ;
    rdfs:label "Censor Reports"@en, "Informes de Lectores"@es, "Informes de Lectors"@ca ;
    rdfs:comment "Official reports written by censors evaluating literary works."@en .

:Editores a owl:Class ;
    rdfs:label "Publishers"@en, "Editores"@es, "Editors"@ca ;
    rdfs:comment "Publishing houses or individuals responsible for publishing books."@en .

:ProveedoresPapel a owl:Class ;
    rdfs:label "Paper Suppliers"@en, "Proveedores de Papel"@es, "Proveïdors de Paper"@ca ;
    rdfs:comment "Suppliers providing paper materials for book printing and publication."@en .

# Supporting Entities
:Coleccion a owl:Class ;
    rdfs:label "Collection"@en, "Colección"@es, "Col·lecció"@ca ;
    rdfs:comment "Thematic or systematic grouping of books or publications."@en .

:Idioma a owl:Class ;
    rdfs:label "Language"@en, "Idioma"@es, "Idioma"@ca ;
    rdfs:comment "Language in which a book or publication is written."@en .

:TachadurasEnmiendas a owl:Class ;
    rdfs:label "Strikethroughs and Amendments"@en, "Tachaduras y Enmiendas"@es, "Ratlles i Esmenes"@ca ;
    rdfs:comment "Editorial marks and corrections made by censors in reviewed texts."@en .

:FirmasLectores a owl:Class ;
    rdfs:label "Censor Signatures"@en, "Firmas de Lectores"@es, "Signatures de Lectors"@ca ;
    rdfs:comment "Official signatures of censors on reviewed documents."@en .

# External Reference Entities
:OtrasBasesDatos a owl:Class ;
    rdfs:label "Other Databases"@en, "Otras Bases de Datos"@es, "Altres Bases de Dades"@ca ;
    rdfs:comment "References to external databases containing related information."@en .

:TextoCompleto a owl:Class ;
    rdfs:label "Full Text"@en, "Texto Completo"@es, "Text Complet"@ca ;
    rdfs:comment "Complete textual content of books or documents."@en .

